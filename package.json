{
  "name": "nginxwebnodeplugin",
  "version": "1.0.8",
  "description": "A nginx proxy server configuration template for webNode.",
  "documentationWebsite": {
    "name": "nginxWebNodePlugin",
    "trackingCode": "TODO"
  },
  "homepage": "http://torben.website/nginxWebNodePlugin",
  "license": "CC-BY-3.0",
  "main": "index.compiled.js",
  "keywords": [
    "web",
    "backend",
    "proxy",
    "server",
    "configuration",
    "template"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/thaibault/nginxWebNodePlugin.git"
  },
  "dependencies": {
    "clientnode": "latest",
    "cmswebnodeplugin": "latest",
    "serverwebnodeplugin": "latest",
    "templatewebnodeplugin": "latest",
    "web-node": "latest"
  },
  "devDependencies": {
    "documentation-website": "latest",
    "qunit-cli": "latest",
    "source-map-support": "latest"
  },
  "scripts": {
    "build": "webOptimizer build",
    "buildDLL": "webOptimizer buildDLL",
    "clear": "webOptimizer clear && rm --force log.txt &>/dev/null",
    "document": "webOptimizer document",
    "lint": "webOptimizer typeCheck && webOptimizer lint",
    "prepublish": "npm run build",
    "serve": "nginx",
    "start": "npm run serve",
    "test": "webOptimizer test",
    "update": "npm version patch && npm publish",
    "updateDocumentation": "documentationWebsite --log-level info",
    "watch": "webOptimizer build --watch",
    "watchDLL": "webOptimizer buildDLL --watch"
  },
  "webNode": {
    "dependencies": [
      "cms",
      "server",
      "template"
    ],
    "server": {
      "proxy": {
        "logFilePath": {
          "access": "/dev/stdout",
          "error": "/dev/stderr"
        },
        "ports": [
          443,
          80
        ]
      }
    }
  }
}

{
  "name": "nginxwebnodeplugin",
  "version": "1.0.52",
  "license": "CC-BY-3.0",
  "description": "A nginx proxy server configuration template for webNode.",
  "homepage": "http://torben.website/nginxWebNodePlugin",
  "documentationWebsite": {
    "name": "nginxWebNodePlugin",
    "trackingCode": "UA-40192634-23"
  },
  "keywords": [
    "web",
    "backend",
    "proxy",
    "server",
    "configuration",
    "template"
  ],
  "main": "index.compiled.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/thaibault/nginxWebNodePlugin.git"
  },
  "scripts": {
    "build": "webOptimizer build",
    "build:dll": "webOptimizer build:dll",
    "clear": "webOptimizer clear",
    "document": "webOptimizer document",
    "lint": "webOptimizer check:type; webOptimizer lint",
    "prepare": "npm run build",
    "report:coverage": "nyc report --reporter=text-lcov | coveralls",
    "serve": "npm run build && webNode",
    "start": "npm run serve",
    "test": "nyc npm run test:plain",
    "test:plain": "webOptimizer test",
    "update": "npm version patch && npm publish",
    "update:documentation": "documentationWebsite --log-level info",
    "watch": "webOptimizer build --watch",
    "watch:dll": "webOptimizer build:dll --watch"
  },
  "peerDependencies": {
    "babel-runtime": "*",
    "serverwebnodeplugin": "*",
    "web-node": "*"
  },
  "devDependencies": {
    "babel-eslint": "^8.1.2",
    "clientnode": "^1.0.315",
    "coveralls": "^3.0.0",
    "documentation-website": "^1.0.88",
    "eslint": "^4.14.0",
    "eslint-config-google": "^0.9.1",
    "eslint-plugin-flowtype": "^2.40.1",
    "eslint-plugin-jsdoc": "^3.3.0",
    "flow-bin": "^0.62.0",
    "jsdoc": "^3.5.2",
    "nyc": "^11.4.1",
    "qunit": "^2.4.1",
    "serverwebnodeplugin": "^1.0.63",
    "source-map-support": "^0.5.0",
    "web-node": "^1.0.137",
    "weboptimizer": "^1.0.458",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.5"
  },
  "webNode": {
    "dependencies": [
      "server"
    ],
    "server": {
      "proxy": {
        "optional": true,
        "logFilePath": {
          "access": "/dev/stdout",
          "error": "/dev/stderr"
        },
        "ports": [
          80,
          443
        ]
      }
    }
  },
  "webOptimizer": {
    "module": {
      "aliases": {
        "clientnode/test$": "clientnode/test.compiled",
        "web-node/configurator$": "web-node/configurator.compiled",
        "web-node/pluginAPI$": "web-node/pluginAPI.compiled"
      },
      "optimizer": {
        "babelMinify": {
          "module": {
            "mangle": false
          }
        }
      }
    }
  }
}

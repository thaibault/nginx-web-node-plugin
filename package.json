{
  "name": "nginxwebnodeplugin",
  "version": "1.0.242",
  "license": "CC-BY-3.0",
  "description": "A nginx proxy server configuration template for webNode.",
  "homepage": "https://torben.website/nginx-web-node-plugin",
  "documentationWebsite": {
    "name": "nginx-web-node-plugin",
    "trackingCode": "UA-40192634-23"
  },
  "keywords": [
    "web",
    "backend",
    "proxy",
    "server",
    "configuration",
    "template"
  ],
  "files": [
    "index.d.ts",
    "index.js",
    "type.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/thaibault/nginx-web-node-plugin.git"
  },
  "scripts": {
    "build": "weboptimizer build",
    "check": "yarn check:types; yarn lint",
    "check:types": "weboptimizer check:types",
    "clear": "weboptimizer clear",
    "document": "weboptimizer document",
    "lint": "weboptimizer lint",
    "prepare": "yarn build",
    "report:coverage": "nyc report --reporter=text-lcov | coveralls",
    "serve": "yarn build && yarn start",
    "start": "web-node '{plugin: {hotReloading: true}}'",
    "test": "weboptimizer test",
    "update": "yarn version --patch && yarn publish",
    "update:documentation": "documentationWebsite --log-level info",
    "watch": "weboptimizer build --watch"
  },
  "devDependencies": {
    "@babel/eslint-parser": "*",
    "@types/jest": "*",
    "@types/node": "*",
    "@types/workbox-webpack-plugin": "*",
    "@typescript-eslint/eslint-plugin": "*",
    "@typescript-eslint/parser": "*",
    "application-server-web-node-plugin": "*",
    "clientnode": "*",
    "documentation-website": "*",
    "eslint": "*",
    "eslint-config-google": "*",
    "eslint-plugin-jsdoc": "*",
    "jest": "*",
    "jsdoc": "*",
    "node-fetch": "*",
    "web-node": "*",
    "weboptimizer": "*"
  },
  "peerDependencies": {
    "@babel/runtime": "*",
    "application-server-web-node-plugin": "*",
    "node-fetch": "*",
    "web-node": "*"
  },
  "peerDependenciesMeta": {
    "@babel/runtime": {
      "optional": true
    }
  },
  "resolutions": {
    "@types/webpack": "https://registry.yarnpkg.com/@favware/skip-dependency/-/skip-dependency-1.1.1.tgz",
    "colors": "1.4.0"
  },
  "sideEffects": false,
  "webNode": {
    "applicationServer": {
      "dependencies": [
        "application-server"
      ],
      "proxy": {
        "optional": true,
        "logFilePath": {
          "access": "/dev/stdout",
          "error": "/dev/stderr"
        },
        "ports": {
          "backend": {
            "encrypted": 443,
            "plain": 80
          }
        }
      }
    }
  },
  "webOptimizer": {
    "commandLine": {
      "test": {
        "environments": [
          "node"
        ]
      }
    },
    "module": {
      "enforceDeduplication": true
    },
    "targetTechnology": "node",
    "test": {
      "injection": {
        "external": {
          "implicit": {
            "pattern": {
              "exclude": {
                "#": "NOTE: Theses files are ECMAscript file so have to be processed to work with test-runner.",
                "__append__": [
                  {
                    "__evaluate__": "/^(.+\\/)?data-uri-to-buffer(\\/.+)?$/"
                  },
                  {
                    "__evaluate__": "/^(.+\\/)?fetch-blob(\\/.+)?$/"
                  },
                  {
                    "__evaluate__": "/^(.+\\/)?node-fetch(\\/.+)?$/"
                  }
                ]
              }
            }
          }
        }
      }
    }
  },
  "dependencies": {
    "@types/webpack": "https://registry.yarnpkg.com/@favware/skip-dependency/-/skip-dependency-1.1.1.tgz"
  }
}

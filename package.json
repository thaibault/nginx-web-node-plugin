{
  "name": "nginxwebnodeplugin",
  "version": "1.0.175",
  "license": "CC-BY-3.0",
  "description": "A nginx proxy server configuration template for webNode.",
  "homepage": "https://torben.website/nginxWebNodePlugin",
  "documentationWebsite": {
    "name": "nginxWebNodePlugin",
    "trackingCode": "UA-40192634-23"
  },
  "keywords": [
    "web",
    "backend",
    "proxy",
    "server",
    "configuration",
    "template"
  ],
  "files": [
    "index.ts"
  ],
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/thaibault/nginxWebNodePlugin.git"
  },
  "scripts": {
    "build": "webOptimizer build",
    "check": "yarn check:types; yarn lint",
    "check:types": "webOptimizer check:types",
    "clear": "webOptimizer clear",
    "document": "webOptimizer document",
    "lint": "webOptimizer lint",
    "prepare": "yarn build",
    "report:coverage": "nyc report --reporter=text-lcov | coveralls",
    "serve": "yarn build && webNode",
    "start": "yarn serve",
    "test": "webOptimizer test",
    "update": "yarn version --patch && yarn publish",
    "update:documentation": "documentationWebsite --log-level info",
    "watch": "webOptimizer build --watch"
  },
  "peerDependencies": {
    "@babel/runtime": "*",
    "serverwebnodeplugin": "*",
    "web-node": "*"
  },
  "devDependencies": {
    "@types/jest": "*",
    "@types/loader-utils": "*",
    "@types/node": "*",
    "@typescript-eslint/eslint-plugin": "*",
    "@typescript-eslint/parser": "*",
    "babel-eslint": "*",
    "clientnode": "*",
    "documentation-website": "*",
    "eslint": "*",
    "eslint-config-google": "*",
    "eslint-plugin-jsdoc": "*",
    "jest": "*",
    "jsdoc": "*",
    "serverwebnodeplugin": "*",
    "web-node": "*",
    "weboptimizer": "*",
    "webpack": "*",
    "webpack-cli": "*",
    "webpack-dev-server": "*"
  },
  "webNode": {
    "dependencies": [
      "server"
    ],
    "server": {
      "proxy": {
        "optional": true,
        "logFilePath": {
          "access": "/dev/stdout",
          "error": "/dev/stderr"
        },
        "ports": [
          80,
          443
        ]
      }
    }
  },
  "sideEffects": false,
  "webOptimizer": {
    "commandLine": {
      "test": {
        "environments": [
          "node"
        ]
      }
    },
    "targetTechnology": "node"
  }
}
